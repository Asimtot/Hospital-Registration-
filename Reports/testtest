        Medication[] mmm1= {b1,b2,b3,b4,b5,s1,r1,r2,r3};

        for(Medication sas: mmm1){
            sas.setRelatedField(orthopedi);
        }
        backPain.setRelatedField(orthopedi);
        sinirSıkışması.setRelatedField(orthopedi);
        romatizma.setRelatedField(orthopedi);

        orthopedi.setHospital(s);

        database.add(orthopedi);

        









